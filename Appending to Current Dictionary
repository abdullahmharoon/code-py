table = {'Sjoerd': 4127, 'Jack': 4098, 'Dcab': 7678, 'Haroon': 9432,}
name=input("Name: ")
phone =int(input("Input: "))
table.update(map(lambda x : (name, phone),range(1)))


for name, phone in table.items():
    print(f'{name:10d} ==> {phone:10d}')

for x in range(1, 110):
    print('{0:2d} {1:2d} {2:4d} {3:5d}'.format(x, x*x, x*x*x,x*x*x*x))
